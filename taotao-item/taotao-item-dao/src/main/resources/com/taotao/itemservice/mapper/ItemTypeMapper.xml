<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.taotao.itemservice.mapper.ItemTypeMapper">
	<select id="findItemTypeByCid" parameterType="long" resultType="String">
		SELECT item_type AS itemType FROM tb_item_type WHERE item_category_id=#{cid}
	</select>
</mapper>